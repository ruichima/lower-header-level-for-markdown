{
	"name": "lower-header-level-for-markdown",
	"displayName": "lower header level for Markdown",
	"description": "A tool for making levels of headers lower in Markdown",
	"version": "1.0.3",
	"publisher": "ruichima",
	"repository": {
		"type": "git",
		"url":"https://github.com/ruichima/markdown-lower-headers-levels"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.lower_header_level_for_markdown"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.lower_header_level_for_markdown",
				"title": "lower header level for Markdown"
			}
		],
		"keybindings": [
			{
				"command": "extension.lower_header_level_for_markdown",
				"key": "ctrl+shift+3"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
